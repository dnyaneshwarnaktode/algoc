/**
 * Angel One SmartAPI Configuration
 * 
 * IMPORTANT: This is an OPTIONAL module for live market data.
 * Paper trading works WITHOUT this configuration.
 * 
 * To enable Angel One integration:
 * 1. Copy this file to .env
 * 2. Fill in your Angel One credentials
 * 3. Set ANGEL_ENABLED=true
 */

// Angel One SmartAPI Settings
ANGEL_ENABLED=false
ANGEL_API_KEY=your_api_key_here
ANGEL_CLIENT_CODE=your_client_code_here
ANGEL_PASSWORD=your_password_here
ANGEL_TOTP_SECRET=your_totp_secret_here

// Market Data Source
// Options: SIMULATED | ANGELONE
MARKET_DATA_SOURCE=SIMULATED

// SmartAPI Settings
ANGEL_RECONNECT_ATTEMPTS=3
ANGEL_RECONNECT_DELAY=5000
ANGEL_SESSION_TIMEOUT=86400000
